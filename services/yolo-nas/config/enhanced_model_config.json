{
  "service": "yolo_nas_enhanced",
  "version": "2.1.0",
  "models": {
    "pose_models": {
      "yolo_nas_pose_s": {
        "model_name": "yolo_nas_pose_s",
        "checkpoint": "coco_pose",
        "version": "v3.7.1",
        "enabled": true,
        "num_classes": 17,
        "task": "pose_estimation",
        "priority": 1
      },
      "yolo_nas_pose_m": {
        "model_name": "yolo_nas_pose_m", 
        "checkpoint": "coco_pose",
        "version": "v3.7.1",
        "enabled": true,
        "num_classes": 17,
        "task": "pose_estimation",
        "priority": 2
      },
      "yolo_nas_pose_l": {
        "model_name": "yolo_nas_pose_l",
        "checkpoint": "coco_pose", 
        "version": "v3.7.1",
        "enabled": false,
        "num_classes": 17,
        "task": "pose_estimation",
        "priority": 3
      }
    },
    "object_models": {
      "yolo_nas_s": {
        "model_name": "yolo_nas_s",
        "checkpoint": "coco",
        "version": "v3.7.1",
        "enabled": true,
        "num_classes": 80,
        "task": "object_detection",
        "priority": 1
      },
      "yolo_nas_m": {
        "model_name": "yolo_nas_m",
        "checkpoint": "coco",
        "version": "v3.7.1", 
        "enabled": false,
        "num_classes": 80,
        "task": "object_detection",
        "priority": 2
      }
    }
  },
  "features": {
    "enhanced_joint_confidence_analysis": {
      "enabled": true,
      "description": "ðŸŽ¯ HIGH PRIORITY: Per-joint confidence scoring and filtering"
    },
    "batch_format_output": {
      "enabled": true,
      "description": "ðŸŽ¯ HIGH PRIORITY: Structured batch predictions for cleaner processing"
    },
    "pose_specific_models": {
      "enabled": true,
      "description": "ðŸŽ¯ HIGH PRIORITY: Dedicated pose models (yolo_nas_pose_s/m/l)"
    },
    "fp16_quantization": {
      "enabled": true,
      "description": "ðŸš€ MEDIUM PRIORITY: 2x faster inference with minimal accuracy loss"
    },
    "custom_nms_padel": {
      "enabled": true,
      "description": "ðŸš€ MEDIUM PRIORITY: Padel-optimized detection parameters"
    },
    "int8_quantization": {
      "enabled": false,
      "description": "ðŸš€ MEDIUM PRIORITY: Maximum speed with padel-specific optimization"
    },
    "17_keypoint_analysis": {
      "enabled": true,
      "description": "ðŸŽ¨ ADVANCED: Full COCO keypoint analysis with padel-specific joints"
    },
    "multi_model_ensemble": {
      "enabled": true,
      "description": "ðŸŽ¨ ADVANCED: Combine pose + object detection for comprehensive analysis"
    },
    "dynamic_pose_quality": {
      "enabled": true,
      "description": "ðŸŽ¨ ADVANCED: Real-time pose quality metrics"
    },
    "onnx_export_pipeline": {
      "enabled": true,
      "description": "ðŸŽ¨ ADVANCED: Complete end-to-end optimized pipeline"
    }
  },
  "thresholds": {
    "confidence_threshold": 0.3,
    "joint_confidence_threshold": 0.3,
    "nms_threshold": 0.5,
    "pose_quality_threshold": 0.6,
    "max_predictions_per_image": 10
  },
  "optimization": {
    "use_half_precision": true,
    "batch_size": 8,
    "enable_fallback": true,
    "preferred_backend": "pytorch",
    "enable_fp16": true,
    "enable_int8": false,
    "use_custom_nms": true,
    "calibration_dataset_size": 100
  },
  "joint_analysis": {
    "enable_joint_tracking": true,
    "stability_window_size": 10,
    "enable_17_keypoint_analysis": true,
    "enable_padel_specific_analysis": true
  },
  "batch_processing": {
    "batch_format": true,
    "output_format": "enhanced_batch",
    "include_metadata": true
  },
  "export_settings": {
    "enable_onnx_export": true,
    "include_preprocessing": true,
    "include_postprocessing": true,
    "quantization_mode": "FP16",
    "export_formats": ["onnx", "tensorrt"]
  },
  "ensemble": {
    "enable_multi_model": true,
    "pose_weight": 0.7,
    "object_weight": 0.3,
    "confidence_aggregation": "weighted_average"
  },
  "padel_specific": {
    "joint_groups": {
      "serving_arm": [5, 7, 9],
      "balance_leg": [11, 13, 15], 
      "racket_grip": [9, 10],
      "core_stability": [5, 6, 11, 12],
      "power_chain": [6, 8, 10],
      "balance_chain": [5, 7, 9]
    },
    "importance_weights": {
      "serving_arm": 1.0,
      "racket_grip": 1.0,
      "power_chain": 0.9,
      "core_stability": 0.9,
      "balance_leg": 0.8,
      "balance_chain": 0.8
    }
  },
  "performance": {
    "confidence_threshold": 0.3,
    "max_concurrent_requests": 4,
    "warmup_iterations": 3,
    "enable_caching": true,
    "cache_size": 100
  }
}