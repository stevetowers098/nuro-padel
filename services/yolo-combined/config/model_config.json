{
  "service": "yolo_combined",
  "version": "2.0.0",
  "models": {
    "yolo11_pose": {
      "file": "yolo11n-pose.pt",
      "version": "v1.0.0",
      "enabled": true,
      "fallback": "yolo8n-pose.pt"
    },
    "yolo11_object": {
      "file": "yolo11n.pt", 
      "version": "v1.0.0",
      "enabled": true,
      "fallback": "yolov8n.pt"
    },
    "yolov8_pose": {
      "file": "yolov8n-pose.pt",
      "version": "v0.9.0", 
      "enabled": true,
      "fallback": null
    },
    "yolov8_object": {
      "file": "yolov8n.pt",
      "version": "v0.9.0",
      "enabled": true,
      "fallback": null
    }
  },
  "features": {
    "enhanced_ball_tracking": {
      "enabled": true,
      "description": "Advanced Kalman filtering for ball trajectory"
    },
    "tracknet_v4": {
      "enabled": false,
      "description": "Next generation TrackNet model (ready for testing)"
    },
    "batch_processing": {
      "enabled": true,
      "description": "Process multiple frames in batches for efficiency"
    },
    "half_precision": {
      "enabled": true,
      "description": "Use FP16 for faster inference on compatible GPUs"
    }
  },
  "performance": {
    "batch_size": 8,
    "confidence_threshold": 0.3,
    "max_concurrent_requests": 5
  }
}